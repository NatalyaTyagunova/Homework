package Lessons1;

public class quest6 {

    public static void main(String[] args) {
        posOrNeg(-100);
        posOrNeg(1);
        posOrNeg(0);
    }

    public static void posOrNeg (int number) {

        if (number <0 ) {
            System.out.println("true");
        }

        else {
            System.out.println("false");
        }
    }
}
