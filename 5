package Lessons1;

public class quest5 {


    public static void main(String[] args) {
        posOrNeg(9);
        posOrNeg(-900);
        posOrNeg(0);
    }

    public static void posOrNeg (int number) {
        if (number < 0) {
            System.out.println("Число " + number + " отрицательное");
        }
            else {
                System.out.println("Число " + number + " положительное");
            }
        }

}
